@{
    var SampleBag = ViewBag.SampleBag;
    var SampleBag_Object_Class = ViewBag.SampleBag_Object_Class;
    var SampleBag_array = ViewBag.SampleBag_array;
}
<h1>ViewBag Page</h1>
<p>this is value of this bag: @SampleBag</p>
<p>this is value of this bag from Object/Class: @SampleBag_Object_Class.SampleBag_value</p>

<!--For Loop-->
@for (int i = 0; i < SampleBag_array.Length; i++)
{
    <p>this is value of this bag from Array: @SampleBag_array[i]</p>
}
