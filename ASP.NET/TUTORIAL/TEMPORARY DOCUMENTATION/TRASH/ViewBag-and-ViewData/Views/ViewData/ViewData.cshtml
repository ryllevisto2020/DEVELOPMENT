@{
    var SampleData = ViewData["SampleData"];
    var SampleData_Object_Class = ViewData["SampleData_Object_Class"] as sampleData;
    var SampleData_array = ViewData["SampleData_array"] as int[];
}
<h1>ViewData Page</h1>
<p>this is value of this Data: @SampleData</p>
<p>this is value of this Data from Object/Class: @SampleData_Object_Class.SampleData_value</p>
<!--For Loop-->
@for (int i = 0; i < SampleData_array.Length; i++)
{
    <p>this is value of this Data from Array: @SampleData_array[i]</p>
}
